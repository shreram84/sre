// ConsoleApplication1.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>

using namespace std;

#include<iostream>
using namespace std;
int main()
{
	int x,y,z;
	cout << " ENTER THE VALUE of X : ";
	cin >> x;
	cout << "ENTER THE VALUE OF Y : ";
	cin >> y;
	z = x;
	x = y;
	y = z;
	cout << " THE VALUE OF X = " << x << endl;
	cout << " THE VALUE OF Y = " << y << endl;
	return 0;
}

