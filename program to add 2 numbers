// ConsoleApplication1.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>

using namespace std;

int main() {
	int a;
	int b;
	int c;
	cout << "Please enter the two numbers to add\n";
	cin >> a >> b;
	c = a + b;
	cout << "The sum of the numbers:  " << c << " " << endl;

}
