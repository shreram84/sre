// ConsoleApplication1.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>

using namespace std;

#include<iostream>
using namespace std;
int main()
{
	int x;
	cout << "Enter the number =  ";
	cin >> x;

	if (x % 2 == 0)
		cout << x << " is even.";
	else
		cout << x << " is odd.";

	return 0;
}
